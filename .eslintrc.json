{
	"env": {
		"browser": true,
		"es6": true,
		"jest/globals": true
	},
	"plugins": ["jest"],
	"extends": [
		"eslint:recommended",
		"next/core-web-vitals",
		"prettier",
		"next"
	],
	"rules": {
		"@next/next/no-img-element": "off",
		"no-unused-vars": [
			"error",
			{
				"argsIgnorePattern": "^_"
			}
		],
		"jest/no-disabled-tests": "error",
		"jest/no-focused-tests": "error",
		"jest/no-identical-title": "error",
		"eqeqeq": ["error", "always"]
	}
}
